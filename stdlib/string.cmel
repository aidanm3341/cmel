// String module - exports native string functions
export var startsWith = __startsWith;
export var endsWith = __endsWith;
export var join = __join;

// reverse is still pure CMEL implementation
export fun reverse(str) {
    var chars = str.split("");
    var result = "";
    for (var i = chars.length() - 1; i >= 0; i = i - 1) {
        result = result + chars[i];
    }
    return result;
}
