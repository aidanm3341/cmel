import suite, test, run, assertEqual from "stdlib/test";

suite("Passing tests", fun() {
    test("should pass", fun() {
        assertEqual(1, 1);
    });
});

suite("Failing tests", fun() {
    test("should fail with wrong value", fun() {
        assertEqual(5, 3);
    });

    test("should fail with wrong type", fun() {
        assertEqual("hello", 42);
    });
});

suite("More passing tests", fun() {
    test("should still run after failures", fun() {
        assertEqual(2 + 2, 4);
    });
});

run();
