// Test list slice native function
import slice from "stdlib/list";

// Basic slice
print slice([1, 2, 3, 4, 5], 1, 3);
// expect: [2, 3]

print slice([1, 2, 3, 4, 5], 0, 5);
// expect: [1, 2, 3, 4, 5]

print slice([1, 2, 3, 4, 5], 2, 4);
// expect: [3, 4]

// Negative indices
print slice([1, 2, 3, 4, 5], -3, -1);
// expect: [3, 4]

print slice([1, 2, 3, 4, 5], -2, 5);
// expect: [4, 5]

print slice([1, 2, 3, 4, 5], 0, -1);
// expect: [1, 2, 3, 4]

// Edge cases
print slice([1, 2, 3], 0, 0);
// expect: []

print slice([1, 2, 3], 3, 3);
// expect: []

print slice([1, 2, 3], 1, 1);
// expect: []

// Out of bounds (should clamp)
print slice([1, 2, 3], 0, 10);
// expect: [1, 2, 3]

print slice([1, 2, 3], -10, 2);
// expect: [1, 2]

// Empty list
print slice([], 0, 0);
// expect: []
