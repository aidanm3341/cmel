// Test built-in List instance methods
// Only import sort since it's the only non-built-in method now
import sort from "stdlib/list";

var numbers = [5, 2, 8, 1, 9];

// Test built-in instance methods
print numbers.sum();
// expect: 25
print numbers.contains(8);
// expect: true
print numbers.contains(100);
// expect: false

fun doubleNum(x) { return x * 2; }
var doubled = numbers.map(doubleNum);
print doubled;
// expect: [10, 4, 16, 2, 18]

fun isEven(x) { return x % 2 == 0; }
var evens = numbers.filter(isEven);
print evens;
// expect: [2, 8]

var sorted = sort(numbers);
print sorted;
// expect: [1, 2, 5, 8, 9]

// Note: reverse() is called on the sorted list since sort() mutates in place
var reversed = numbers.reverse();
print reversed;
// expect: [9, 8, 5, 2, 1]
