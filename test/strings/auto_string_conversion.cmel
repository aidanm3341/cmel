// Test automatic string conversion with + operator

// String + Number
print "Answer: " + 42;
// expect: Answer: 42

// Number + String
print 100 + " bottles";
// expect: 100 bottles

// String + Boolean (true)
print "Boolean: " + true;
// expect: Boolean: true

// String + Boolean (false)
print "Value: " + false;
// expect: Value: false

// String + Nil
print "Nil: " + nil;
// expect: Nil: nil

// Multiple concatenations
print "x=" + 5 + ", y=" + 10;
// expect: x=5, y=10

// Float conversion
print "Pi: " + 3.14;
// expect: Pi: 3.14

// Concatenation with variables
var num = 999;
var str = "Number: " + num;
print str;
// expect: Number: 999

// Edge case: empty string
print "" + 123;
// expect: 123

// Number first, then string, then number
print 1 + " item " + 2;
// expect: 1 item 2
